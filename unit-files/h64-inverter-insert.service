[Unit]
Description=Unit file for running h64 family home data collection tools - inverter insert into DB

[Service]
User=yourusername
Group=yourusername
ExecStart=/usr/bin/python3 /home/yourusername/h64/inverter/insert_deye_data_into_db.py &> /var/log/h64/insert_deye_data_into_db
Restart=on-failure

[Install]
WantedBy=multi-user.target
